{
  "name": "sfdx-easy-sources",
  "description": "This plugin provides tools to simplify Salesforce sources",
  "version": "0.7.7",
  "author": "Raffaele Prudenzano @raffo93p",
  "bugs": "https://github.com/raffo93p/sfdx-easy-sources/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@salesforce/command": "^5",
    "@salesforce/core": "^3",
    "csv": "^6.2.1",
    "csvtojson": "^2.0.10",
    "dottie": "^2.0.2",
    "fs-extra": "^10.1.0",
    "json2csv": "^5.0.7",
    "prompt-sync": "^4.2.0",
    "tslib": "^2",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@oclif/test": "^2",
    "@salesforce/ts-sinon": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "eslint-plugin-header": "^3",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsdoc": "^35",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-typescript": "^0",
    "mocha": "^9",
    "nyc": "^15.1.0",
    "oclif": "^3",
    "prettier": "^2",
    "sinon": "10.0.0",
    "ts-node": "^10",
    "typescript": "4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/raffo93p/sfdx-easy-sources",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    }
  },
  "repository": "raffo93p/sfdx-easy-sources",
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint src/**/*.ts test/**/*.ts",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "rm -rf test_tmp && mkdir test_tmp && cd test_tmp && nyc --extension .ts --require ts-node/register mocha --forbid-only \"../test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "link": "npm install && npm run build && sf plugins link"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  }
}
